{
  "name": "portfolio",
  "source": "public/index.html",
  "version": "1.0.0",
  "description": "A simple portfolio website",
  "scripts": {
    "start": "parcel",
    "build": "npx tailwindcss -i src/input.css -o public/portfolio.css && parcel build public/index.html",
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf .parcel-cache",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "keywords": [
    "portfolio",
    "website",
    "react",
    "tailwindcss",
    "lucide",
    "parcel",
    "javascript"
  ],
  "author": "Dhanesh Katre",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@tailwindcss/cli": "^4.0.5",
    "lucide-react": "^0.475.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.5"
  },
  "devDependencies": {
    "parcel": "^2.13.3",
    "process": "^0.11.10"
  }
}
